platform :ios, '9.0'

target ‘RealEstate’ do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod ‘GoogleMaps’
  pod ‘GooglePlaces’

end
